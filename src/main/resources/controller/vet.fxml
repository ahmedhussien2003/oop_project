<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SplitMenuButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="87.0" prefWidth="171.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.VetController">
   <children>
      <HBox layoutX="-1.0" onMouseClicked="#click" prefHeight="87.0" prefWidth="753.0" style="-fx-background-color: #99A1A6;">
         <children>
            <Label fx:id="vetname" alignment="CENTER" prefHeight="86.0" prefWidth="167.0" text="Ahmed">
               <font>
                  <Font name="Cambria Bold" size="20.0" />
               </font>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <padding>
                  <Insets left="20.0" />
               </padding>
            </Label>
            <Label fx:id="vetid" alignment="CENTER" prefHeight="86.0" prefWidth="164.0" text="123456">
               <font>
                  <Font name="Cambria Bold" size="19.0" />
               </font>
            </Label>
            <Label fx:id="vetpassword" alignment="CENTER" prefHeight="87.0" prefWidth="186.0" text="123456789">
               <font>
                  <Font name="Cambria" size="20.0" />
               </font>
            </Label>
            <SplitMenuButton fx:id="vetpet" alignment="CENTER" mnemonicParsing="false" popupSide="LEFT" prefHeight="66.0" prefWidth="194.0" text="15 pet">
              <items>
                <MenuItem mnemonicParsing="false" text="Action 1" />
                <MenuItem mnemonicParsing="false" text="Action 2" />
              </items>
               <font>
                  <Font name="Arial Black" size="28.0" />
               </font>
               <HBox.margin>
                  <Insets top="7.0" />
               </HBox.margin>
            </SplitMenuButton>
         </children>
      </HBox>
   </children>
</AnchorPane>
